{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "src/pages/[...slug].js": {
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
} 